{
  "name": "Messenger Dark Theme",
  "version": "1.0.1",
  "description": "Dark Theme for Messenger",
  "manifest_version": 2,
  "permissions": ["declarativeContent"],
  "content_scripts": [
    {
      "matches": ["*://*.messenger.com/*"],
      "css": ["css/injected.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon.jpg",
    "32": "images/icon.jpg",
    "48": "images/icon.jpg",
    "128": "images/icon.jpg"
  }
}
